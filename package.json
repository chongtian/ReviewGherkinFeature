{
    "name": "p2landreviewfeature",
    "displayName": "P2 Land Review Feature File",
    "description": "Review feature file following P2 Land team's guideline",
    "version": "0.0.1",
    "publisher": "TonyGao",
    "repository": {
        "type": "git",
        "url": "https://github.com/chongtian/ReviewP2LandFeature.git"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.reviewFeature"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.reviewFeature",
                "title": "Review Feature"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}